[access "refs/heads/*"]
abandon = group zone-opendev.org-core
label-Code-Review = -2..+2 group zone-opendev.org-core
label-Workflow = -1..+1 group zone-opendev.org-core

[access "refs/tags/*"]
pushSignedTag = group zone-opendev.org-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
