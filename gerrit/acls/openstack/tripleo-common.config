[access "refs/heads/*"]
abandon = group tripleo-common-core
label-Code-Review = -2..+2 group tripleo-common-core
label-Workflow = -1..+1 group tripleo-common-core

[access "refs/tags/*"]
pushSignedTag = group tripleo-common-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
